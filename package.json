    "@rollup/plugin-babel": "5.3.1",
    "@types/react": "17.0.43",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "rollup": "2.70.1",
    "rollup-plugin-copy": "3.4.0",
