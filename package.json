  "files": [
    "src/",
    "dist/"
  ],
  "sideEffects": "false",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brophdawg11/remix-validity-state.git"
  },
  "keywords": [
    "remix",
    "form",
    "input",
    "validation",
    "validitystate",
    "validity"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brophdawg11/remix-validity-state/issues"
  },
